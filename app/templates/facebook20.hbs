<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&appId=161923053887892&version=v2.0";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<noscript>
  <p>
    <a class="noBorder"
       href="https://www.facebook.com/{{username}}">
      Facebook page from {{username}}
    </a>
  </p>
</noscript>
{{#if show_head_follow}}
<div class="head-follow facebook-head-follow">
  <div class="head-title">
    <span class="head-icon"></span>
    Facebook
  </div>
  <div class="follow-button fb-like"
       data-href="https://developers.facebook.com/docs/plugins/"
       data-layout="button"
       data-action="like"
       data-show-faces="false"
       data-share="false"></div>
</div>
{{/if}}
<div class="facebook-content pane feed">
  <div class="fb-like-box"
       data-href="http://www.facebook.com/{{username}}"
       {{#if width}}
       data-width="{{width}}"
       {{/if}}
       {{#if height}}
       data-height="{{height}}"
       {{/if}}
       data-colorscheme="{{colorscheme}}"
       data-show-faces="{{show_faces}}"
       data-header="{{header}}"
       data-stream="{{stream}}"
       data-show-border="{{show_border}}">
  </div>
</div>
<noscript>
  <div class="error">
    <p>
      <a class="noBorder"
         href="https://www.facebook.com/{{username}}">
        Facebook page from {{username}}
      </a>
    </p>
  </div>
</noscript>
