<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/pt_BR/sdk.js#xfbml=1&version=v2.3";
  fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>
<noscript>
  <p>
    <a class="noBorder"
       href="https://www.facebook.com/{{username}}">
      Facebook page from {{username}}
    </a>
  </p>
</noscript>
{{#if show_head_follow}}
<div class="head-follow facebook-head-follow">
  <div class="head-title">
    <span class="head-icon"></span>
    Facebook
  </div>
  <div class="follow-button fb-like"
       data-href="https://developers.facebook.com/docs/plugins/"
       data-layout="button"
       data-action="like"
       data-show-faces="false"
       data-share="false"></div>
</div>
{{/if}}
<div class="facebook-content pane feed">
  <div class="fb-page"
       data-href="https://www.facebook.com/{{username}}"
       {{#if width}}
       data-width="{{width}}"
       {{/if}}
       {{#if height}}
       data-height="{{height}}"
       {{/if}}
       data-hide-cover="{{hide_cover}}"
       data-show-facepile="{{show_facepile}}"
       data-show-posts="{{show_posts}}"
       data-hide-cta="{{hide_cta}}"
       data-small-header="{{small_header}}"
       data-adapt-container-width="{{adapt_container_width}}">
    <div class="fb-xfbml-parse-ignore">
      <blockquote cite="https://www.facebook.com/{{username}}">
        <a href="https://www.facebook.com/{{username}}">
          Facebook page from {{username}}
        </a>
      </blockquote>
    </div>
  </div>
</div>
<noscript>
  <div class="error">
    <p>
      <a class="noBorder"
         href="https://www.facebook.com/{{username}}">
        Facebook page from {{username}}
      </a>
    </p>
  </div>
</noscript>
